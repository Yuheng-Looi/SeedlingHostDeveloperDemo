{
  "template": [
    {
      "type": "card-template",
      "attr": {
        "entry": "notification",
        "category": "symmetry"
      },
      "events": {
        "click": "clickBtn"
      },
      "children": [
        {
          "type": "image",
          "attr": {
            "level": "0:A*",
            "src": "{{symmetry_logo}}"
          }
        },
        {
          "type": "text",
          "attr": {
            "level": "1:B*",
            "value": "{{symmetry_additional}}"
          }
        },
        {
          "type": "div",
          "attr": {
            "level": "5:F",
            "bgImage": "{{symmetry_bg}}"
          },
          "children": [
            {
              "type": "div",
              "attr": {
                "level": "6-1:G1",
                "bgImage": "{{symmetry_bg}}"
              },
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "level": "0-1:A1*",
                    "src": "{{symmetry_logo}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "2-1:C1",
                    "value": "{{symmetry_name}}"
                  }
                }
              ]
            },
            {
              "type": "text",
              "attr": {
                "level": "1-1:B1*",
                "value": "{{symmetry_content}}"
              }
            },
            {
              "type": "text",
              "attr": {
                "level": "2-2:C2*",
                "value": "{{symmetry_number}}"
              }
            },
            {
              "type": "text",
              "attr": {
                "level": "2-3:C3",
                "value": "{{symmetry_checkIn}}"
              }
            },
            {
              "type": "div",
              "attr": {
                "level": "6:G"
              },
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "level": "2-4:C4",
                    "value": "{{symmetry_departureLocation}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "2-5:C5",
                    "value": "{{symmetry_destLocation}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-1:D1",
                    "value": "{{symmetry_departureTime}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-2:D2",
                    "value": "{{symmetry_destTime}}"
                  }
                },
                {
                  "type": "image",
                  "attr": {
                    "level": "3-3:D3",
                    "src": "{{symmetry_airplane}}"
                  }
                },
                {
                  "type": "text",
                  "attr": {
                    "level": "3-4:D4",
                    "value": "{{symmetry_extraDays}}"
                  }
                }
              ]
            },
            {
              "type": "button",
              "attr": {
                "level": "4:E",
                "type": "capsule",
                "value": "{{symmetryBtnText}}"
              },
              "events": {
                "click": "clickButton"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "card-template",
      "attr": {
        "entry": "desktop"
      },
      "events": {
        "click": "clickBtn"
      },
      "children": [
        {
          "type": "div",
          "attr": {},
          "classList": [
            "container"
          ],
          "children": [
            {
              "type": "div",
              "attr": {},
              "classList": [
                "top"
              ],
              "children": [
                {
                  "type": "image",
                  "attr": {
                    "src": "assets/images/logo.png"
                  },
                  "classList": [
                    "icon"
                  ]
                },
                {
                  "type": "text",
                  "attr": {
                    "value": "{{title}}"
                  },
                  "classList": [
                    "text",
                    "title"
                  ]
                }
              ]
            },
            {
              "type": "div",
              "attr": {},
              "classList": [
                "center"
              ],
              "children": [
                {
                  "type": "div",
                  "attr": {},
                  "classList": [
                    "bigCard"
                  ],
                  "children": [
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/Scan.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{describe}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/QR.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{describe1}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/nfc.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{describe2}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    },
                    {
                      "type": "div",
                      "attr": {},
                      "classList": [
                        "message"
                      ],
                      "children": [
                        {
                          "type": "image",
                          "attr": {
                            "src": "assets/images/Frame 4.png"
                          },
                          "classList": [
                            "image"
                          ]
                        },
                        {
                          "type": "text",
                          "attr": {
                            "value": "{{describe3}}"
                          },
                          "classList": [
                            "text",
                            "describe"
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "data": {
    "title": "Pay with OPPO Pay",
    "describe": "Scan",
    "describe1": "QR Code",
    "describe2": "NFC",
    "describe3": "Transfer",
    "symmetry_logo": "assets/images/logo.png",
    "symmetry_additional": "Received RM 40.00 from Tey Henry",
    "symmetry_content": "RM 40.00",
    "symmetry_airplane": "assets/images/transferring.png",
    "symmetry_departureTime": "-RM 40.00",
    "symmetry_destTime": "+RM 40.00",
    "symmetry_extraDays": "+1",
    "symmetry_bg": "assets/images/symmetry_bg.png",
    "symmetry_departureLocation": "Tey Henry",
    "symmetry_destLocation": "YOU",
    "symmetry_number": "RECEIVED from Tey Henry",
    "symmetry_checkIn": "Just now",
    "symmetryBtnText": "Report",
    "symmetry_name": "OPPO Pay",
    "symmetry_serviceName": "航旅纵横"
  },
  "actions": {
    "clickBtn": {
      "type": "deeplink",
      "uri": "com.oplus.seedling.action.taxi",
      "data": "uri",
      "params": {
        "method": "train_click",
        "travelId": "{{title}}",
        "trainNumber": "{{describe}}"
      }
    }
  },
  "styles": {
    ".container": {
      "width": "100%",
      "height": "100%",
      "paddingTop": "16px",
      "paddingRight": "22px",
      "paddingBottom": "0px",
      "paddingLeft": "22px",
      "flexDirection": "column",
      "backgroundColor": "#ffffff"
    },
    ".top": {
      "minHeight": "20px",
      "alignItems": "center",
      "marginBottom": "24px"
    },
    ".text": {
      "fontFamily": "sys-sans-en",
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.85)"
    },
    ".title": {
      "marginLeft": "6px"
    },
    ".icon": {
      "width": "20px",
      "height": "20px"
    },
    ".center": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".bigCard": {
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    ".image": {
      "width": "40px",
      "height": "40px"
    },
    ".message": {
      "flexDirection": "column",
      "justifyContent": "space-between",
      "alignItems": "center"
    },
    ".describe": {
      "fontWeight": "400",
      "fontSize": "14px",
      "color": "rgba(0,0,0,0.6)"
    },
    ".middleCard": {
      "display": "none",
      "width": "100%",
      "height": "64px",
      "justifyContent": "space-between"
    },
    "@MEDIA": [
      {
        "condition": "(aspect-ratio: 1*2)",
        ".container": {
          "paddingTop": "12px",
          "paddingRight": "24px",
          "paddingBottom": "0px",
          "paddingLeft": "24px"
        },
        ".top": {
          "display": "none"
        },
        ".image": {
          "width": "32px",
          "height": "32px"
        },
        ".bigCard": {
          "display": "none"
        },
        ".middleCard": {
          "display": "flex",
          "height": "53.1px"
        },
        ".describe": {
          "fontSize": "12px"
        }
      },
      {
        "condition": "(aspect-ratio: 2*2)",
        ".container": {
          "paddingTop": "16px",
          "paddingRight": "16px",
          "paddingBottom": "0px",
          "paddingLeft": "16px"
        },
        ".bigCard": {
          "display": "none"
        },
        ".middleCard": {
          "display": "flex"
        }
      }
    ]
  },
  "apiVersion": {}
}